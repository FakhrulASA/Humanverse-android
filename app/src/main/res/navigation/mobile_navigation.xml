<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.humanverse.humanverseapp.feature.home.ui.ui.ui.home.HomeFragment"
        android:label="Humanverse"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.humanverse.humanverseapp.feature.home.ui.ui.ui.dashboard.DashboardFragment"
        android:label="History"
        tools:layout="@layout/fragment_dashboard" />
    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.humanverse.humanverseapp.feature.home.ui.ui.ui.notifications.NotificationsFragment"
        android:label="Profile"
        tools:layout="@layout/fragment_notifications" />
    <fragment
        android:id="@+id/navigation_history"
        android:name="com.humanverse.humanverseapp.feature.home.ui.ui.ui.service.ServiceFragment"
        android:label="@string/title_history"
        tools:layout="@layout/history_fragment" />
</navigation>